{
    "name": "euclid1990/ios-emoji-php",
    "description": "An iOS Emoji Parser For Laravel 5",
    "license": "MIT",
    "authors": [
        {
            "name": "Nguyen Van Vuong",
            "email": "euclid1990@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "illuminate/config": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1"
    },
    "autoload": {
        "psr-4": {
            "euclid1990\\PhpIosEmoji\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ],
        "psr-4": {
            "euclid1990\\PhpIosEmoji\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
